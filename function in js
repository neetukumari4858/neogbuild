 2
 let toSentenceCase=(a)=>{
     let s=" "
     k=a.split(" ")
     for (i of k){
         s=`${s+i.charAt(0).toUpperCase()}${ i.slice(1)} `;
     }
     return s
 }
 console.log(toSentenceCase('we are neoGrammers'))5

 3 Accending order
 let sortArray=(a)=>{
     let max=0;
     for (i of a){
         if (i>max){
             max=i
         }
     }
     let l=max
     let l1=[]
     for (let j=0; j<=l; j++){
         if (a.includes(j)){
             l1.push(j)
         }
     }
     console.log(l1)
 }
 sortArray([100,83,32,9,45,61])


 // decending order
 let sortArray=(a)=>{
     let max=0;
     for (i of a){
         if (i>max){
             max=i
         }
     }
     let l=max
     let l1=[]
     for (let j=l; j>=1; j--){
         if (a.includes(j)){
             l1.push(j)
         }
     }
     console.log(l1)
 }
 sortArray([100,83,32,9,45,61])

