 3
 let noOfWords= (a) =>{ let m=a.split(" ") 
     let k=m.length
     return k
 }
 console.log(noOfWords("We are neoGrammers"));

 4
 let findMin=(...a)=> {
     let min=a[0];
     for (i of a){
         if (i<min){
             min=i
         }
     }
     return min
 }
 console.log(findMin(3,5,9,1))

 5

 let findMax=(...a)=> {
     let max=0;
     for (i of a){
         if (i>max){
             max=i
         }
     }
     return max
 }
 console.log(findMax(3,5,9,1))

 6
 scalene, isosceles, equilateral  
 let typeOfTriangle=(...a)=>{
     if (a[0]==a[1]==a[2]){
         console.log("equilateral traingle")
     }
     else if (a[0]!=a[1] || a[1]!=a[2] || a[2]!=a[1]){
         console.log("isosceles")
     }
     else{
         console.log("scalene")
     }
 }
 typeOfTriangle(30, 30, 30)

