 let indexOf=(a,b)=>{
     for (let i=0; i<a.length;i++){
         if (a[i]===b){
             console(i)
         }
     }
 }
 console.log(indexOf([1,6,3,5,8,9], 5))

3
 [1,10,3,10,6,8]
 let replace=(a,b,c)=>{
     for (i of a){
         if (i==b){
             let k=a.indexOf(i)
             a.splice(k,1,c)
         }
     }
     return a
 }
 console.log(replace([1,5,3,5,6,8], 5, 10))

